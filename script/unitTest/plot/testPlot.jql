/*================================

Test Plot command
	
================================*/

sz = 400;

// tests that no stroke is drawn when not set
t = select i,
	#0000ff80 styleFill,
	i % 2 == 0 ? #ff0000 : null styleStroke,
	i % 4 == 0 ? 4 : null styleStrokeWidth,
	Geom.buffer(Geom.createPoint(x, x), radius)
	with {
		radius = sz / 10.0;
		x = i * radius;
	}
	from Generate.sequence(1,10);

Plot data: t file: "plotDot.png";


	
	
