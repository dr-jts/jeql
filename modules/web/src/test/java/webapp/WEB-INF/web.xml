<web-app xmlns="http://java.sun.com/xml/ns/javaee"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
  version="2.5">
  <display-name>JEQL</display-name>
    <servlet>
      <servlet-name>JeqlServlet</servlet-name>
      <servlet-class>jeql.JeqlServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>JeqlServlet</servlet-name>
        <url-pattern>*.jql</url-pattern>
    </servlet-mapping>
</web-app>

